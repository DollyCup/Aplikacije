<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="TableSat">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">@dimen/table_cell_height</item>
        <item name="android:background">@drawable/border</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_column">1</item>
    </style>

    <style name="TablePredmet">
        <item name="android:layout_width">@dimen/table_cell_width</item>
        <item name="android:layout_height">@dimen/table_cell_height</item>
        <item name="android:background">@drawable/border</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_column">2</item>
    </style>


    <style name="TableVrijeme">
        <item name="android:layout_width">@dimen/table_cell_width</item>
        <item name="android:layout_height">@dimen/table_cell_height</item>
        <item name="android:background">@drawable/border</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_column">3</item>
    </style>

</resources>
